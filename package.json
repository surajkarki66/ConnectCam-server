{
  "name": "ConnectCam",
  "version": "1.0.0",
  "description": "This is a video calling web application where you can connect with your friends and family",
  "main": "index.js",
  "scripts": {
    "build": "rm -rf dist/ && tsc",
    "start:dev": "nodemon --watch './**/*.ts' --exec 'ts-node --files' src/index.ts",
    "start:prod": "node ./dist/index.js",
    "format": "prettier --write \"src/**/*.ts\""
  },
  "repository": "https://github.com/surajkarki66/ConnectCam",
  "author": "surajkarki66 <suraj.karki500@protonmail.com>",
  "license": "Apache-2.0",
  "private": false,
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/node": "^20.14.2",
    "nodemon": "^3.1.3",
    "prettier": "^3.3.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.4.5"
  },
  "dependencies": {
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "socket.io": "^4.7.5"
  }
}
